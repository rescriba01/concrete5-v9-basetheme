<?xml version="1.0"?>
<styles version="1.0">
    <webfonts>
        <skin identifier="night-road">
            <font type="google" name="Abril Fatface" />
            <font type="google" name="Oxygen" />
            <font type="google" name="Oswald" />
        </skin>
        <skin identifier="default">
            <font type="google" name="Titillium Web" />
        </skin>
    </webfonts>
    <set name="Colors">
        <style name="Primary Color" variable="primary" type="color"/>
        <style name="Secondary Color" variable="secondary" type="color"/>
    </set>
    <set name="Typography">
        <style name="Heading 1" variable="h1" type="type" />
        <style name="Heading 2" variable="h2" type="type" />
        <style name="Heading 3" variable="h3" type="type" />
        <style name="Heading 4" variable="h4" type="type" />
        <style name="Heading 5" variable="h5" type="type" />
        <style name="Heading 6" variable="h6" type="type" />
    </set>
    <set name="Header">
        <style
                name="Logo"
                variable="logo"
                type="type"
        />
    </set>
    <set name="Image Slider">
        <style
                name="Background Image"
                variable="image-slider-background-image"
                type="image"
        />
        <style
                name="Title"
                variable="image-slider-title"
                type="type"
        />
    </set>
    <set name="Miscellaneous">
        <style name="Thumbnail Hover Color" variable="thumbnail-hover" type="color" />
        <style name="Accent Background" variable="accent-bg" type="color" />
        <style name="Accent Text Color" variable="accent-color" type="color" />
    </set>
</styles>